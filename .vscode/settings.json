{
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.environment": {
        "VCPKG_BINARY_SOURCES": "clear;files,${workspaceFolder}/../thirdparty/vcpkg-cache,readwrite",
